$group

    function TheoryList
        param props
        var {jsonData} = props
        return
            div
                < h2 Concepts
                < Paper
                    @ style {{maxHeight: 370, overflow: 'auto'}}
                    < Table 
                        {
                            _ jsonData.theories.sort(compareById).map
                                function
                                    param item
                                    return
                                        < TableRow 
                                            @ button
                                            @ key {item.id}
                                            < TableCell
                                                h3 {item.id}
                                                < ContentText
                                                    @ item {item}

    
    function Theory
        param props
        var {item} = props
        return
            < div 
                < h3 {item.id}
                < ContentText
                    @ item {item}
        
